<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.autodoor.autoscrew.ActivityPara"

    >




        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="X轴速度"
            android:id="@+id/textView75"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="false"
            android:layout_alignWithParentIfMissing="false"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="5dp"
            android:layout_alignParentEnd="false"
            android:textSize="18dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Y轴速度"
            android:id="@+id/textView76"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="10dp"
            android:layout_below="@+id/textView75"
            android:layout_alignParentStart="true"
            android:layout_marginTop="5dp"
            android:textSize="18dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Z轴速度"
            android:id="@+id/textView77"
            android:layout_below="@+id/textView76"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="5dp"
            android:textSize="18dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="取螺丝Z轴速度"
            android:id="@+id/textView78"
            android:minWidth="100dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="10dp"
            android:layout_below="@+id/textView77"
            android:layout_alignParentStart="true"
            android:layout_marginTop="5dp"
            android:textSize="18dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="锁螺丝Z轴速度"
            android:id="@+id/textView79"
            android:layout_below="@+id/textView78"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="5dp"
            android:textSize="18dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="取螺丝时间"
            android:id="@+id/textView80"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="10dp"
            android:layout_below="@+id/textView79"
            android:layout_alignParentStart="true"
            android:layout_marginTop="5dp"
            android:textSize="18dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="锁螺丝时间"
            android:id="@+id/textView81"
            android:layout_below="@+id/textView80"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="5dp"
            android:textSize="18dp" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etSpeedX"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="10dp"
            android:minWidth="60dp"
            android:gravity="right"
            android:singleLine="false"
            android:layout_alignBaseline="@+id/textView75"
            android:layout_toRightOf="@+id/textView78" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etSpeedY"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="10dp"
            android:minWidth="60dp"
            android:layout_alignBaseline="@+id/textView76"
            android:gravity="right"
            android:layout_toRightOf="@+id/textView78" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etSpeedZ"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="10dp"
            android:minWidth="60dp"
            android:gravity="right"
            android:layout_alignBaseline="@+id/textView77"
            android:layout_toRightOf="@+id/textView78" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etSpeedZGetScrew"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="10dp"
            android:minWidth="60dp"
            android:layout_toEndOf="@+id/textView78"
            android:gravity="right"
            android:layout_toRightOf="@+id/textView78"
            android:layout_alignBaseline="@+id/textView78" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etSpeedZLockScrew"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="10dp"
            android:minWidth="60dp"
            android:gravity="right"
            android:layout_alignBaseline="@+id/textView79"
            android:layout_toRightOf="@+id/textView78" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etGetScrewTime"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="10dp"
            android:minWidth="60dp"
            android:gravity="right"
            android:layout_toRightOf="@+id/textView78"
            android:layout_alignBaseline="@+id/textView80" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etScrewLooseTime"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="10dp"
            android:minWidth="60dp"
            android:gravity="right"
            android:layout_alignBaseline="@+id/textView81"
            android:layout_toRightOf="@+id/textView78" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="螺丝长度(mm)"
            android:id="@+id/textView82"
            android:layout_alignWithParentIfMissing="false"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="15dp"
            android:layout_toEndOf="@+id/etSpeedY"
            android:layout_toRightOf="@+id/etSpeedX"
            android:layout_alignBaseline="@+id/etSpeedY"
            android:textSize="18dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="取螺丝前高度(mm)"
            android:id="@+id/textView83"
            android:layout_alignWithParentIfMissing="false"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="15dp"
            android:layout_alignBaseline="@+id/etSpeedZ"
            android:layout_toEndOf="@+id/etSpeedY"
            android:layout_toRightOf="@+id/etSpeedX"
            android:textSize="18dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="锁螺丝力度"
            android:id="@+id/textView84"
            android:layout_alignWithParentIfMissing="false"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="15dp"
            android:layout_toEndOf="@+id/etSpeedZ"
            android:layout_toRightOf="@+id/etSpeedX"
            android:layout_alignBaseline="@+id/etSpeedZGetScrew"
            android:textSize="18dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="圆弧1(取螺丝端)"
            android:id="@+id/textView85"
            android:layout_alignWithParentIfMissing="false"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="15dp"
            android:layout_toEndOf="@+id/etSpeedZLockScrew"
            android:layout_toRightOf="@+id/etGetScrewTime"
            android:layout_alignBaseline="@+id/etGetScrewTime"
            android:textSize="18dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="圆弧2(锁螺丝端)"
            android:id="@+id/textView86"
            android:layout_alignWithParentIfMissing="false"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="15dp"
            android:layout_alignBaseline="@+id/etScrewLooseTime"
            android:layout_toRightOf="@+id/etGetScrewTime"
            android:textSize="18dp" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etLeftBoxGetScrewHeight"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="0dp"
            android:minWidth="60dp"
            android:gravity="right"
            android:singleLine="false"
            android:layout_alignBaseline="@+id/textView83"
            android:layout_toRightOf="@+id/textView83" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etLeftBoxScrewLength"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="0dp"
            android:minWidth="60dp"
            android:gravity="right"
            android:singleLine="false"
            android:layout_alignBaseline="@+id/textView82"
            android:layout_toRightOf="@+id/textView83" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etLeftBoxForceDegree"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="0dp"
            android:minWidth="60dp"
            android:gravity="right"
            android:singleLine="false"
            android:layout_alignBaseline="@+id/textView84"
            android:layout_toRightOf="@+id/textView83" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etRightBoxScrewLength"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="0dp"
            android:minWidth="60dp"
            android:gravity="right"
            android:singleLine="false"
            android:layout_toEndOf="@+id/etLeftBoxGetScrewHeight"
            android:layout_alignBaseline="@+id/textView82"
            android:layout_toRightOf="@+id/etLeftBoxScrewLength" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etRightBoxGetScrewHeight"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="0dp"
            android:minWidth="60dp"
            android:gravity="right"
            android:singleLine="false"
            android:layout_alignBaseline="@+id/etLeftBoxGetScrewHeight"
            android:layout_toRightOf="@+id/etLeftBoxGetScrewHeight" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etRightBoxForceDegree"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="0dp"
            android:minWidth="60dp"
            android:gravity="right"
            android:singleLine="false"
            android:layout_alignBaseline="@+id/textView84"
            android:layout_toRightOf="@+id/etLeftBoxForceDegree" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="左仓"
            android:id="@+id/textView87"
            android:layout_alignWithParentIfMissing="false"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:textStyle="bold"
            android:layout_alignStart="@+id/etLeftBoxScrewLength"
            android:layout_alignBaseline="@+id/etSpeedX"
            android:layout_toRightOf="@+id/textView83" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="右仓"
            android:id="@+id/textView88"
            android:layout_alignWithParentIfMissing="false"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:textStyle="bold"
            android:layout_alignBaseline="@+id/etSpeedX"
            android:layout_alignStart="@+id/etRightBoxScrewLength" />

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etArcGetScrewSide"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="0dp"
            android:minWidth="60dp"
            android:gravity="right"
            android:singleLine="false"
            android:layout_toRightOf="@+id/textView83"
            android:layout_alignBaseline="@+id/textView85" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/etArcLockScrewSide"
            android:text="0"
            android:padding="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="0dp"
            android:minWidth="60dp"
            android:gravity="right"
            android:singleLine="false"
            android:layout_toRightOf="@+id/textView83"
            android:layout_alignBaseline="@+id/textView86" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="复位"
            android:id="@+id/btnReset"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:minWidth="120dp" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="运行"
            android:id="@+id/btnRun"
            android:layout_alignParentEnd="true"
            android:layout_below="@+id/btnReset"
            android:minWidth="120dp" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="循环"
            android:id="@+id/btnCirculation"
            android:layout_alignParentEnd="true"
            android:layout_below="@+id/btnRun"
            android:minWidth="120dp" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="返回"
            android:id="@+id/btnReturn"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="保存"
            android:id="@+id/btnSave"
            android:layout_alignParentBottom="true"
            android:layout_toStartOf="@+id/btnReturn" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toStartOf="@+id/btnSave"
        android:layout_alignParentBottom="true"
        android:layout_alignTop="@+id/btnSave"
        android:gravity="center_vertical|right"
        android:layout_alignEnd="@+id/tbArcOnOrOff">

        <ToggleButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New ToggleButton"
            android:id="@+id/tbD"
            android:textOn="电批运转"
            android:textOff="电批停止" />

        <ToggleButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New ToggleButton"
            android:id="@+id/tbE"
            android:textOff="电磁铁关"
            android:textOn="电磁铁开" />

        <ToggleButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New ToggleButton"
            android:id="@+id/tbA"
            android:textOff="不检螺丝"
            android:textOn="检测螺丝" />

        <ToggleButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New ToggleButton"
            android:id="@+id/tbB"
            android:textOn="检测刹车"
            android:textOff="不检刹车" />
    </LinearLayout>

    <ToggleButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New ToggleButton"
        android:id="@+id/tbArcOnOrOff"
        android:textOn="圆弧开启"
        android:textOff="圆弧关闭"
        android:layout_alignStart="@+id/etRightBoxForceDegree"
        android:layout_below="@+id/textView79"
        android:layout_marginTop="10dp"
        android:checked="true" />


</RelativeLayout>
